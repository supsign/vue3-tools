{
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:vue/vue3-essential",
		"plugin:@typescript-eslint/recommended"
	],
	"overrides": [],
	"parser": "vue-eslint-parser",
	"parserOptions": {
		"parser": "@typescript-eslint/parser",
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"vue",
		"@typescript-eslint"
	],
	"rules": {
		"key-spacing": "error",
		"arrow-spacing": 2,
		"object-curly-spacing": [
			2,
			"always"
		],
		"indent": [
			"error",
			"tab"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"never"
		],
		"space-infix-ops": "error",
		"vue/define-props-declaration": [
			"error",
			"type-based"
		],
		"vue/define-emits-declaration": [
			"error",
			"type-based"
		],
		"vue/v-for-delimiter-style": [
			"error",
			"in"
		]
	}
}